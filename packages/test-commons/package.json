{
    "name": "@blueprintjs/test-commons",
    "version": "3.0.1",
    "description": "Common utilities for tests in @blueprintjs packages",
    "type": "module",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": "./lib/index.js",
        "./bootstrap": "./bootstrap.mjs"
    },
    "scripts": {
        "compile": "tsc -p ./src",
        "clean": "rm -rf lib/* || true",
        "dev": "yarn compile --watch",
        "lint": "run-p lint:es",
        "lint:es": "es-lint",
        "lint-fix": "es-lint --fix",
        "test": "exit 0"
    },
    "dependencies": {
        "assert": "^2.1.0",
        "chai": "^5.0.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.7",
        "mocha": "^10.2.0"
    },
    "peerDependencies": {
        "react": ">=16.8",
        "react-dom": ">=16.8"
    },
    "devDependencies": {
        "@blueprintjs/node-build-scripts": "workspace:^",
        "@types/assert": "^1.5.10",
        "@types/chai": "~4.3.11",
        "@types/enzyme": "~3.10.18",
        "@types/enzyme-adapter-react-16": "~1.0.9",
        "@types/mocha": "~10.0.6",
        "@types/node": "~20.11.6",
        "@types/react": "~18.2.18",
        "@types/react-dom": "~16.9.24",
        "npm-run-all": "^4.1.5",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "typescript": "~5.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:palantir/blueprint.git",
        "directory": "packages/test-commons"
    },
    "author": "Palantir Technologies",
    "license": "Apache-2.0"
}
